{
  "name": "product",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "number",
      "required": true
    },
    "description": "string",

    "created": "date"
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "ownerId"
    },
    "productPicture": {
      "type": "hasMany",
      "model": "productPicture",
      "foreignKey": "product_id"
    }
  },
  "acls": [],
  "methods": {}
}
